{
  "name": "probee-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build-css && vite",
    "build": "npm run build-css && tsc -b && vite build",
    "build-css": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/main.css",
    "build-css:watch": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/main.css --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
